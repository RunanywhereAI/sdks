{
  "name": "@runanywhere/vad-silero",
  "version": "1.0.0",
  "description": "Silero VAD adapter for RunAnywhere Web SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker": "./dist/vad.worker.js",
    "./assets/*": "./dist/assets/*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@runanywhere/core": "workspace:*"
  },
  "dependencies": {
    "@ricky0123/vad-web": "^0.0.24"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vite": "^6.0.6"
  },
  "keywords": [
    "vad",
    "voice-activity-detection",
    "silero",
    "runanywhere",
    "web-ai"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runanywhere/web-sdk.git",
    "directory": "packages/vad-silero"
  },
  "license": "MIT"
}

{
  "name": "@runanywhere/workers",
  "version": "0.1.0",
  "description": "Web Workers implementation for RunAnywhere Web Voice Pipeline",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./workers/*": {
      "types": "./dist/workers/*.d.ts",
      "import": "./dist/workers/*.esm.js",
      "require": "./dist/workers/*.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "runanywhere",
    "workers",
    "webworkers",
    "performance",
    "audio",
    "ai"
  ],
  "author": "RunAnywhere AI",
  "license": "MIT",
  "dependencies": {
    "@runanywhere/core": "workspace:*",
    "@runanywhere/transcription": "workspace:*",
    "@runanywhere/voice": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "typescript": "^5.7.2",
    "vite": "^5.2.11"
  },
  "peerDependencies": {
    "@ricky0123/vad-web": "^0.0.19",
    "@xenova/transformers": "^2.17.2"
  }
}
